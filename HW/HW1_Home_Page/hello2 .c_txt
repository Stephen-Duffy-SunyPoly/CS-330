#include <sys/time.h>
struct timeval tv;
void main()             //main function declaration
{
    int x= gettimeofday(&tv, 0);
    printf("Hello World %d", (int)tv.tv_usec);      //to output the string on a display
    getchar ();             //terminating function
    extern FILE* stdin;
    getc (stdin);           //terminating function
}
